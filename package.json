{
  "name": "@react-navigation/core",
  "version": "3.0.2",
  "description": "Core utilities for the react-navigation framework",
  "main": "dist/index.js",
  "react-native": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "src/",
    "types/",
    "LICENSE.md",
    "README.md"
  ],
  "scripts": {
    "pretest": "yarn lint",
    "test": "jest",
    "lint": "tslint --project tsconfig.json",
    "prebuild": "rimraf dist",
    "build": "tsc --project tsconfig.build.json",
    "prepare": "yarn build",
    "release": "release-it"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "react-navigation",
    "routing",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-navigation/react-navigation-core.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/react-navigation/react-navigation-core/issues"
  },
  "homepage": "https://github.com/react-navigation/react-navigation-core#readme",
  "dependencies": {
    "@types/hoist-non-react-statics": "^3.0.1",
    "@types/query-string": "^6.2.0",
    "@types/react-lifecycles-compat": "^3.0.0",
    "create-react-context": "0.2.2",
    "hoist-non-react-statics": "^3.2.1",
    "path-to-regexp": "^1.7.0",
    "query-string": "^6.2.0",
    "react-is": "^16.7.0",
    "react-lifecycles-compat": "^3.0.4"
  },
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "@types/react": "^16.7.13",
    "@types/react-is": "^16.5.0",
    "@types/react-native": "^0.57.17",
    "@types/react-test-renderer": "^16.0.3",
    "conventional-changelog-cli": "^2.0.5",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.8.2",
    "pretty-quick": "^1.8.0",
    "react": "16.3.1",
    "react-native": "^0.55.4",
    "react-test-renderer": "16.3.1",
    "release-it": "^7.6.1",
    "rimraf": "^2.6.3",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2"
  },
  "peerDependencies": {
    "react": "*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src}/**/*.ts*": [
      "yarn lint",
      "git add"
    ]
  }
}
